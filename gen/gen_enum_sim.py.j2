import enum

{% macro genenum(enum) %}
class {{ enum.name }}(enum.IntEnum):
{% for val in enum['values'] %}
    {{ val.x_name }} = {{ val.value | replace(' ', '') }}
{% endfor %}
{% endmacro %}


{% for header in headers %}
{% for enum in header.enums %}
{{ genenum(enum) }}
{% endfor %}
{% for cls in header.classes %}
{% for enum in cls.enums.public %}
{{ genenum(enum) }}
{% endfor %}
{% endfor %}
{% endfor %}
